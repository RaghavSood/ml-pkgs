# langchain/default.nix generated by extract-langchain-deps.nix

{ lib
, buildPythonPackage
, fetchFromGitHub
, poetry-core
{% for item in inputs -%}
, {{ item }}
{% endfor %}}:

buildPythonPackage rec {
  pname = "langchain";
  version = "{{ version }}";

  src = fetchFromGitHub {
    owner = "hwchase17";
    repo = "langchain";
    rev = "v${version}";
  };

  format = "pyproject";

  buildInputs = [
    poetry-core
  ];

  propagatedBuildInputs = let
    core = [
      {% for item in core -%}
      {{ item }}
      {% endfor %}
    ];
    
    core-utils = [
      {% for item in core_utils -%}
      {{ item }}
      {% endfor %}
    ];
  in core ++ core-utils;

  pythonImportsCheck = [
    "langchain"
  ];

  meta = with lib; {
    description = ''
      Building applications with LLMs through composability
    '';
    homepage = "https://docs.langchain.com/docs/";
    license = licenses.mit;
    maintainers = with maintainers; [ breakds ];
  };
}
